# ----------------------------------------------------------
# Lambda Test
#
# Triggering Events : Belief,Goal
# Body              : Belief,Goal,Action
# Arguments         : Number,String,Name,List,Tuple,Variable
# Condition         : Yes
# Lambda            : Yes
#
# ----------------------------------------------------------

# -------------------------
# No arguments, empty body 
# -------------------------

(+belief() | ( c_belief_1(_("X"))  
                & (lambda : not (X<100) and (X>2) or (X==4) and not (X==2) ) ) ) 
>> 
    [
    
    
    ]

#(-belief() | ( c_belief_1() & c_belief_2() ) ) >> []
#
#(+~goal() | ( c_belief_1() & c_belief_2() & c_belief_3() ) ) >> []
#
#(-~goal() | ( c_belief_1() & c_belief_2() & c_belief_3() & c_belief_4() ) ) 
#>> []
